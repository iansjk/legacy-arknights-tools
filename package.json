{
  "name": "arknights-tools",
  "description": "A collection of tools for Arknights, a tower defense mobile game by Hypergryph/Yostar",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "start": "yarn develop",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "ak:update": "yarn ak:update:img && yarn ak:update:ops && yarn ak:update:items && yarn ak:update:recruitment",
    "ak:update:ops": "git submodule update --remote && ts-node-script scripts/operators.ts",
    "ak:update:img": "git submodule update --remote && ts-node-script scripts/images.ts",
    "ak:update:items": "git submodule update --remote && ts-node-script scripts/items.ts",
    "ak:update:recruitment": "git submodule update --remote && ts-node-script scripts/recruitment.ts",
    "netlify:deploy": "yarn ak:update && yarn build && mv public arknights && mkdir public && mv arknights public"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/node": "^14.14.14",
    "axios": "^0.21.1",
    "eslint-plugin-prettier": "^3.3.0",
    "gatsby": "^2.26.1",
    "gatsby-plugin-layout": "^1.7.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-source-filesystem": "^2.8.0",
    "gatsby-theme-material-ui": "^1.0.13",
    "gatsby-transformer-json": "^2.8.0",
    "js-combinatorics": "^1.4.5",
    "material-ui-popup-state": "^1.7.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "2.1.2",
    "typescript": "^3.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iansjk/arknights-tools"
  },
  "bugs": {
    "url": "https://github.com/iansjk/arknights-tools/issues"
  },
  "engines": {
    "node": ">=12"
  }
}
